@model IEnumerable<Model.DTO.DTO_Ad.DTO_Item_Type>
@using Model.DTO.DTO_Ad;
@{
   var typed= (string)Session["typeProduct"];
}

<select style="width:200px;margin-left:10%" name="typeProduct" id="typeProduct" required>
    <option selected>@typed</option>

    @foreach (DTO_Item_Type item in Model)
    {
        <option>@item.Type_Product</option>
    }
</select>
